/* rs-theme-name: Flat Spring Onion (Zoe) */
/* rs-theme-is-dark: FALSE */
/* Flat Spring Onion
 *
 * Based on the "Flat White" Atom Syntax Theme
 * By Dmitry Biletskyy
 * https://github.com/biletskyy/flatwhite-syntax
 * Color palette
 * https://coolors.co/f6f9f1-bad098-598329-2f6524-1b2e1b-061102-494547-353134
 */

.ace_editor, .ace_editor_theme .profvis-flamegraph, .ace_editor_theme {
  background-color: #F6F9F1;
  color: #203C1E;
}

.ace_gutter {
  background: #E0EAD0;
  color: #7F9F56;
}

.ace_gutter-active-line {
  background-color: #BDD39C;
}

.terminal {
  background-color:#E0EAD0;
  color: #7F9F56;
}

.ace_cursor {
  color: #7FBA3B;
}

.ace_marker-layer .ace_selection {
  background: #BFD9BF;
  border: 2px solid #9AC19A;
  margin: 0 0 0 -2px;
}

.ace_marker-layer .ace_active-line {
  background: rgba(216, 233, 210, 0.5)
}

.ace_bracket {
  margin: 0 !important;
  border: 0 !important;
  background-color: #92E873;
}

.ace_keyword {
  color: #52BA12;
  font-weight: bold;
}

.ace_meta,
.ace_support.ace_constant.ace_property-value {
  color: #494547
}

.ace_constant {
  color: #2F6524;
}

.ace_constant.ace_language {
  color: inherit;
  background-color: #CAFAA0;
  font-style: italic;
}

.ace_constant.ace_numeric {
  color: inherit;
  background-color: #BFE8B0;
  font-style: italic;
}

.ace_string {
  color: #2F6524;
  background-color: #DCF2A7;
  font-style: italic;
}

.ace_indent-guide {
  display: inline-block;
  height: 100%;
  box-shadow: 1px 0 0 #D8DECE;
}

.ace_comment {
  font-style: italic !important;
  color: #62AF46;
}

.ace_print-margin {
  width: 2px;
  background: #D8E9D2
}

/* roxygen keywords */

.ace_keyword.ace_virtual-comment {
  background-color: #F7FFC2;
  color: #6CB950 !important;
}

/* errors */

.GNTYACMBK3B {
  background-color: #FFDBE7 !important;
  color: #2F6524 !important;
}

/* ---- Rainbow indent stuff ---- */

.rstudio_rainbow_indent_guides .ace_line .ace_indent-guide:nth-child(2n+1){
    background: linear-gradient(to left, #8DC358 1px, transparent 1px, transparent);
}
.rstudio_rainbow_indent_guides .ace_line .ace_indent-guide:nth-child(2n+2){
    background: linear-gradient(to left, #B3E37D 1px, transparent 1px, transparent);
}

.rstudio_rainbow_indent_fills .ace_line .ace_indent-guide:nth-child(2n+1){
    background: linear-gradient(to left, #D7EDC0 1px, #D7EDC0 1px, #D7EDC0);
}
.rstudio_rainbow_indent_fills .ace_line .ace_indent-guide:nth-child(2n+2){
    background: linear-gradient(to left, #EBF6DF 1px, #EBF6DF 1px, #EBF6DF);
}

#rstudio_console_input .ace_marker-layer .ace_selection {
  background: #8FEF6C;
}

/* Rainbow Parenthesis */
.ace_paren_color_0 {
  color: #309C94;
}

.ace_paren_color_1 {
  color: #30819C;
}

.ace_paren_color_2 {
  color: #30529C;
}

.ace_paren_color_3 {
  color: #36309C;
}

.ace_paren_color_4 {
  color: #5D309C;
}

.ace_paren_color_5 {
  color: #93309C;
}

.ace_paren_color_6 {
  color: #9C305B;
}

/* ---- RStudio Theme ---- */

.rstudio-themes-flat .rstudio-themes-default {
  background: #B4CC8E !important;
  color: #1B2E1B !important;
}

/* inactive tabs */

table.rstheme_tabLayoutCenter,
.rstudio-themes-flat .rstudio-themes-default .gwt-TabLayoutPanelTabs,
.rstheme_multiPodUtilityTabArea {
  background:  #DAE9C3 !important;
}

table.rstheme_tabLayoutCenter .gwt-Label,
.rstudio-themes-flat .rstudio-themes-default .gwt-TabLayoutPanelTabs .gwt-Label,
.rstheme_multiPodUtilityTabArea .gwt-Label {
