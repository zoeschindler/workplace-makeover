}

.rstudio-themes-flat #rstudio_workbench_panel_find_in_files td[colspan="2"] {
  color: #7FBA3B;
}

.rstheme_secondaryToolbar, .rstudio-themes-flat .dataGridHeader {
  background: #A8C297 !important;
}

/* Data Frame Preview */

.dataTables_wrapper thead th {
  background-color: #A8C297 !important;
  color: #203C1E !important;
}

.dataTables_scrollBody table tbody td:first-child {
  background: #D8E5C5 !important;
  color: #203C1E !important;
}

.dataTables_info {
  background: #A8C297 !important;
  color: #203C1E !important;
}

/* scrollbars */

/* reset scrollbar background color */

.rstudio-themes-flat .ace_scroller {
  overflow: visible;
}

.rstudio-themes-flat, .rstudio-themes-flat .rstudio-themes-scrollbars {
  scrollbar-width: auto;
  scrollbar-color: #61814B #A8C297;
}

/* fix overly zealous css rules */

.gwt-DialogBox-ModalDialog .dialogContent .gwt-Label {
  color: #61814B;
}

/* job progress bar */

.rstheme_toolbarWrapper [role="progressbar"] > div, .windowframe [role="progressbar"] > div {
  background-color: #61814B;
}

/* Rmd Chunk Headers */

.ace_support.ace_function.ace_codebegin, .ace_support.ace_function.ace_codeend {
  font-style: italic;
  font-weight: 600;
}

/* Rmd inline code */

[class="ace_support ace_function"] {
  color: #4D673C;
  background-color: #BAD098;
}

/* Rmd bold and italics */

.ace_constant.ace_numeric.ace_text {
  font-weight: 600;
}

.ace_constant.ace_language.ace_boolean.ace_text {
  font-style: italic;
}

.ace_url {
  color: #4D673C;
  background-color: #CFD7C2;
}

.ace_meta.ace_tag + .ace_keyword {
  color: #61814B;
  background-color: inherit;
}

/* Fix RStudio toolbars and things */

.rstudio-themes-default > div:last-child > div > div > .rstheme_toolbarWrapper {
  border-color: transparent;
}

.rstudio-themes-default > div:last-child > div > div > .rstheme_toolbarWrapper table {
  background-color: #A8C297;
}

.rstudio-themes-flat .rstudio-themes-default .rstudio-themes-border,
.rstudio-themes-flat .windowframe > div:last-child,
.rstudio-themes-flat .rstudio-themes-default .rstheme_toolbarWrapper,
.rstudio-themes-flat .rstudio-themes-default .rstheme_secondaryToolbar,
.rstudio-themes-flat .rstudio-themes-default .gwt-TabLayoutPanel > div > div.gwt-TabLayoutPanelTabs {
  border-color: #DDE9D3 !important;
}

